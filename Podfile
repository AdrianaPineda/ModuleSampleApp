platform :ios, '12.1'
use_frameworks!

target 'TruoraModuleSampleApp' do
    # both core and doc
    #pod "iOSCoreModule"

    # only core
    pod "iOSCoreModule/iOSCoreOnlyModule"
    #pod "iOSCoreModule/iOSCommonModule" only used if common is not @_implementationOnly

    # only doc
    #pod "iOSCoreModule/iOSDocModule"
end

